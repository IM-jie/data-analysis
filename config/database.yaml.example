# ClickHouse数据库连接配置
clickhouse:
  host: localhost
  port: 9000
  database: analytics
  user: default
  password: ""
  
  # 连接池配置
  pool_size: 10
  max_overflow: 20
  
  # 超时配置
  connect_timeout: 10
  send_receive_timeout: 30
  
  # SSL配置（可选）
  secure: false
  verify: false
  
  # 压缩配置
  compression: true
